.jcarousel-skin-milkbox .jcarousel-container {
    -moz-border-radius: 10px;
   
}

.jcarousel-skin-milkbox .jcarousel-direction-rtl {
	direction: rtl;
}

.jcarousel-skin-milkbox .jcarousel-container-horizontal {
    width: 500px;
    padding: 5px 40px;

}

.jcarousel-skin-milkbox .jcarousel-container-vertical {
    width: 75px;
    height: 245px;
    padding: 40px 20px;
}

.jcarousel-skin-milkbox .jcarousel-clip {
    overflow: hidden;
}

.jcarousel-skin-milkbox .jcarousel-clip-horizontal {
    width: 500px;
    height: 320px;
}

.jcarousel-skin-milkbox .jcarousel-clip-vertical {
    width:  75px;
    height: 245px;
}

.jcarousel-skin-milkbox .jcarousel-item {
    width: 75px;
    height: 320px;
}

.jcarousel-skin-milkbox .jcarousel-item .popup-img{
    width: 560px;
    height: 315px;
    display: table-cell;
    text-align: center;
    vertical-align: middle;
}

.jcarousel-skin-milkbox .jcarousel-item .popup-img img{
    max-height: 250px;
    max-width: 500px;
}

#facebox .popup-imgs .popup-thumbs{
    text-align: center;
}

#facebox .popup-imgs .popup-thumbs img{
    max-height: 90px;
    max-width: 90px;
    margin: 5px;
}


.jcarousel-skin-milkbox .jcarousel-item-horizontal {
	margin-left: 0;
    margin-right: 10px;
}

.jcarousel-skin-milkbox .jcarousel-direction-rtl .jcarousel-item-horizontal {
	margin-left: 10px;
    margin-right: 0;
}

.jcarousel-skin-milkbox .jcarousel-item-vertical {
    margin-bottom: 10px;
}

.jcarousel-skin-milkbox .jcarousel-item-placeholder {
    background: #fff;
    color: #000;
}

/**
 *  Horizontal Buttons
 */
.jcarousel-skin-milkbox .jcarousel-next-horizontal {
    position: absolute;
    top: 115px;
    right: 5px;
    width: 32px;
    height: 32px;
    cursor: pointer;
    background:url(slide-right-active.png) center center no-repeat transparent;
}

.jcarousel-skin-milkbox .jcarousel-direction-rtl .jcarousel-next-horizontal {
    left: 5px;
    right: auto;
    background-image: url(prev-horizontal.png);
}

.jcarousel-skin-milkbox .jcarousel-next-horizontal:hover,
.jcarousel-skin-milkbox .jcarousel-next-horizontal:focus {
    background:url(slide-right-active.png) center center no-repeat transparent;
}

.jcarousel-skin-milkbox .jcarousel-next-horizontal:active {
    background:url(slide-right-active.png) center center no-repeat transparent;
}

.jcarousel-skin-milkbox .jcarousel-next-disabled-horizontal,
.jcarousel-skin-milkbox .jcarousel-next-disabled-horizontal:hover,
.jcarousel-skin-milkbox .jcarousel-next-disabled-horizontal:focus,
.jcarousel-skin-milkbox .jcarousel-next-disabled-horizontal:active {
    cursor: default;
    background:url(slide-right-inactive.png) center center no-repeat transparent;
}

.jcarousel-skin-milkbox .jcarousel-prev-horizontal {
    position: absolute;
    top: 115px;
    left: 5px;
    width: 32px;
    height: 32px;
    cursor: pointer;
    background:url(slide-left-active.png) center center no-repeat transparent;
}

.jcarousel-skin-milkbox .jcarousel-direction-rtl .jcarousel-prev-horizontal {
    left: auto;
    right: 5px;
    background-image: url(next-horizontal.png);
}

.jcarousel-skin-milkbox .jcarousel-prev-horizontal:hover, 
.jcarousel-skin-milkbox .jcarousel-prev-horizontal:focus {
    background:url(slide-left-active.png) center center no-repeat transparent;
}

.jcarousel-skin-milkbox .jcarousel-prev-horizontal:active {
    background:url(slide-left-active.png) center center no-repeat transparent;
}

.jcarousel-skin-milkbox .jcarousel-prev-disabled-horizontal,
.jcarousel-skin-milkbox .jcarousel-prev-disabled-horizontal:hover,
.jcarousel-skin-milkbox .jcarousel-prev-disabled-horizontal:focus,
.jcarousel-skin-milkbox .jcarousel-prev-disabled-horizontal:active {
    cursor: default;
    background:url(slide-left-inactive.png) center center no-repeat transparent;
}

/**
 *  Vertical Buttons
 */
.jcarousel-skin-milkbox .jcarousel-next-vertical {
    position: absolute;
    bottom: 5px;
    left: 43px;
    width: 32px;
    height: 32px;
    cursor: pointer;
    background: transparent url(next-vertical.png) no-repeat 0 0;
}

.jcarousel-skin-milkbox .jcarousel-next-vertical:hover,
.jcarousel-skin-milkbox .jcarousel-next-vertical:focus {
    background-position: 0 -32px;
}

.jcarousel-skin-milkbox .jcarousel-next-vertical:active {
    background-position: 0 -64px;
}

.jcarousel-skin-milkbox .jcarousel-next-disabled-vertical,
.jcarousel-skin-milkbox .jcarousel-next-disabled-vertical:hover,
.jcarousel-skin-milkbox .jcarousel-next-disabled-vertical:focus,
.jcarousel-skin-milkbox .jcarousel-next-disabled-vertical:active {
    cursor: default;
    background-position: 0 -96px;
}

.jcarousel-skin-milkbox .jcarousel-prev-vertical {
    position: absolute;
    top: 5px;
    left: 43px;
    width: 32px;
    height: 32px;
    cursor: pointer;
    background: transparent url(prev-vertical.png) no-repeat 0 0;
}

.jcarousel-skin-milkbox .jcarousel-prev-vertical:hover,
.jcarousel-skin-milkbox .jcarousel-prev-vertical:focus {
    background-position: 0 -32px;
}

.jcarousel-skin-milkbox .jcarousel-prev-vertical:active {
    background-position: 0 -64px;
}

.jcarousel-skin-milkbox .jcarousel-prev-disabled-vertical,
.jcarousel-skin-milkbox .jcarousel-prev-disabled-vertical:hover,
.jcarousel-skin-milkbox .jcarousel-prev-disabled-vertical:focus,
.jcarousel-skin-milkbox .jcarousel-prev-disabled-vertical:active {
    cursor: default;
    background-position: 0 -96px;
}
