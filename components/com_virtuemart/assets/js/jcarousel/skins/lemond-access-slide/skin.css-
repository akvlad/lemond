.jcarousel-skin-lemond-access-slide .jcarousel-container {
    background-color: transparent;
    border: none;
}

.jcarousel-skin-lemond-access-slide .jcarousel-direction-rtl {
	direction: rtl;
}

.jcarousel-skin-lemond-access-slide .jcarousel-container-horizontal {
	width:901px; /*903+80=983*/
	height:205px; /*175+40=215*/
	padding: 10px 38px 0 38px;
	margin:0 auto;
}

.jcarousel-skin-lemond-access-slide .jcarousel-container-vertical {
    width: 75px;
    height: 245px;
    padding: 40px 20px;
}

.jcarousel-skin-lemond-access-slide .jcarousel-clip {
    overflow: hidden;
}

.jcarousel-skin-lemond-access-slide .jcarousel-clip-horizontal {
    width: 901px;
    height: 205px;
}
.jcarousel-skin-lemond-access-slide .jcarousel-clip-horizontal .jcarousel-list-horizontal
{
	width:  935px;
    height: 205px;
}
.jcarousel-skin-lemond-access-slide .jcarousel-clip-vertical {
    width:  75px;
    height: 245px;
}

.jcarousel-skin-lemond-access-slide .jcarousel-item {
	width: 125px;
	height: 205px;
}
.jcarousel-skin-lemond-access-slide .jcarousel-item a
{
	color: #999999;
	font-size: 11px;
	line-height: 14px;	
}
.jcarousel-skin-lemond-access-slide .jcarousel-item-horizontal {
	margin-left: 0;
    margin-right: 30px;
	
}

.jcarousel-skin-lemond-access-slide .jcarousel-direction-rtl .jcarousel-item-horizontal {
	margin-left: 10px;
    margin-right: 0;
}

.jcarousel-skin-lemond-access-slide .jcarousel-item-vertical {
    margin-bottom: 10px;
}

.jcarousel-skin-lemond-access-slide .jcarousel-item-placeholder {
    background: #fff;
    color: #000;
}

/**
 *  Horizontal Buttons
 */
.jcarousel-skin-lemond-access-slide .jcarousel-next-horizontal {
    position: absolute;
    top: 60px;
    right: 5px;
    width: 32px;
    height: 32px;
    cursor: pointer;
    background: transparent url(next-horizontal.png) no-repeat 0 0;
}

.jcarousel-skin-lemond-access-slide .jcarousel-direction-rtl .jcarousel-next-horizontal {
    left: 5px;
    right: auto;
    background: url(slide-right-active.png) top left  no-repeat transparent;
}

.jcarousel-skin-lemond-access-slide .jcarousel-next-horizontal:hover,
.jcarousel-skin-lemond-access-slide .jcarousel-next-horizontal:focus {
    background-position: -32px 0;
}

.jcarousel-skin-lemond-access-slide .jcarousel-next-horizontal:active {
    background-position: -64px 0;
}

.jcarousel-skin-lemond-access-slide .jcarousel-next-disabled-horizontal,
.jcarousel-skin-lemond-access-slide .jcarousel-next-disabled-horizontal:hover,
.jcarousel-skin-lemond-access-slide .jcarousel-next-disabled-horizontal:focus,
.jcarousel-skin-lemond-access-slide .jcarousel-next-disabled-horizontal:active {
    cursor: default;
    background: url(slide-right-inactive.png) top left  no-repeat transparent;
}

.jcarousel-skin-lemond-access-slide .jcarousel-prev-horizontal {
    position: absolute;
    top: 60px;
    left: 5px;
    width: 32px;
    height: 32px;
    cursor: pointer;
    background: transparent url(prev-horizontal.png) no-repeat 0 0;
}

.jcarousel-skin-lemond-access-slide .jcarousel-direction-rtl .jcarousel-prev-horizontal {
    left: auto;
    right: 5px;
    background: url(slide-left-active.png) top left  no-repeat transparent;
}

.jcarousel-skin-lemond-access-slide .jcarousel-prev-horizontal:hover, 
.jcarousel-skin-lemond-access-slide .jcarousel-prev-horizontal:focus {
    background-position: -32px 0;
}

.jcarousel-skin-lemond-access-slide .jcarousel-prev-horizontal:active {
    background-position: -64px 0;
}

.jcarousel-skin-lemond-access-slide .jcarousel-prev-disabled-horizontal,
.jcarousel-skin-lemond-access-slide .jcarousel-prev-disabled-horizontal:hover,
.jcarousel-skin-lemond-access-slide .jcarousel-prev-disabled-horizontal:focus,
.jcarousel-skin-lemond-access-slide .jcarousel-prev-disabled-horizontal:active {
    cursor: default;
    background: url(slide-left-inactive.png) top left  no-repeat transparent;
}

/**
