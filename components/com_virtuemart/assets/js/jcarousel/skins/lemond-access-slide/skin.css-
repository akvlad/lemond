}

.jcarousel-skin-lemond-access-slide .jcarousel-next-disabled-vertical,
.jcarousel-skin-lemond-access-slide .jcarousel-next-disabled-vertical:hover,
.jcarousel-skin-lemond-access-slide .jcarousel-next-disabled-vertical:focus,
