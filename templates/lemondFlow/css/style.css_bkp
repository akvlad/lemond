/**************CART-POPUP*****************/

#facebox .popup 
{

	border: none;
	-webkit-border-radius: 30px;
	-moz-border-radius: 30px;
	border-radius: 30px;
	-webkit-box-shadow: 0 0 18px rgba(0,0,0,0.4);
	-moz-box-shadow: 0 0 18px rgba(0,0,0,0.4);
	box-shadow: 0 0 18px rgba(0,0,0,0.4);
}
#facebox .content 
{
	padding:0;
	-webkit-border-radius: 30px;
	-moz-border-radius: 30px;
	border-radius: 30px;

}

#facebox .content object
{
	width: 470px;
	height: 250px;
}

#facebox .content  #vm_popup_wrapper
{
	width:618px;
	-webkit-border-radius: 30px;
	-moz-border-radius: 30px;
	border-radius: 30px;
}
#facebox .content  #vm_popup_wrapper  .prod-item
{
	height:192px;
	position:relative;
	border-bottom:1px solid #e1f5ff;
}
#facebox .content  #vm_popup_wrapper #products_wrapper
{
	background:url(../images/prod-cart-list-fon.png) bottom center no-repeat #ffffff;
	position:relative;
}
#facebox .content #div-h2-popup-cart
{
	height:40px;
	background-color:#e6e6e6;
	-webkit-border-radius: 30px 30px 0 0;
	-moz-border-radius: 30px 30px 0 0;
	border-radius: 30px 30px 0 0;
}
#facebox .content   #div-h2-popup-cart h2
{
	margin:0;
	text-align:center;
	font-size:18px;
	font-weight:normal;
	line-height: 40px;
	color:#1A556E;
}

#facebox .content  #vm_popup_wrapper #products_wrapper .prod-item .delete-product
{
	position:absolute;
	top:75px;
	left:5px;
}
#facebox .content  #vm_popup_wrapper #products_wrapper .prod-item .div-prod-image
{
	height:144px;
	width:147px;
	display:table;
	vertical-align: middle;
	background:url(../images/prod-card-fon.png);
	position:absolute;
	left:55px;
	top:25px;
}
#facebox .content  #vm_popup_wrapper #products_wrapper .prod-item .div-prod-image img
{
	display: block;
	margin: 0 auto;
	max-height:144px;
	max-width:147px;
}

#facebox .content  #vm_popup_wrapper #products_wrapper .prod-item .div-prod-name
{
	position:absolute;
	top:25px;
	left:237px;
	font-size:14px;
	color:#712636;
	text-transform: uppercase;
}
#facebox .content  #vm_popup_wrapper #products_wrapper .prod-item .div-prod-count
{
	position:absolute;
	width:40px;
	height:52px;
	bottom:25px;
	left:230px;
	background:url(../images/prod-num.png);
}
#facebox .content  #vm_popup_wrapper #products_wrapper .prod-item .div-prod-count span
{
	position: absolute;
