/*---------HEADER------------------------*/
#middle #top-line
{
	height:20px;
	width:100%;
	background: #6699ff; /* ��� ������ �������� */
    background: -moz-linear-gradient(left, #6699ff, #eaa5b6); /* Firefox 3.6+ */
    /* Chrome 1-9, Safari 4-5 */
    background: -webkit-gradient(linear, left, right, 
                color-stop(0%,#6699ff), color-stop(100%,#eaa5b6));
    /* Chrome 10+, Safari 5.1+ */
    background: -webkit-linear-gradient(left, #6699ff, #eaa5b6);
    background: -o-linear-gradient(left, #6699ff, #eaa5b6); /* Opera 11.10+ */
    background: -ms-linear-gradient(left, #6699ff, #eaa5b6); /* IE10 */
    background: linear-gradient(left, #6699ff, #eaa5b6); /* CSS3 */ 
}

#middle #top-line #top-menu
{
